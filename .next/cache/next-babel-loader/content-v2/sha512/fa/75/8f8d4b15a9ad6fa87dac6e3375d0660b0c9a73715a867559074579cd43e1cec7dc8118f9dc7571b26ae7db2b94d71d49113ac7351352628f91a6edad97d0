                required\n                type={showPassword ? \"password\" : \"text\"}\n                label=\"Senha\"\n                name=\"password\"\n                value={password}\n                onChange={handleInput}\n              />\n            </div>\n\n            <div className=\"login-eye-icon d-flex justify-content-end\">\n              {showPassword ? (\n                <FaEye\n                  onClick={() => setShowPassword(!showPassword)}\n                  className=\"mb-3\"\n                  size=\"20\"\n                />\n              ) : (\n                <FaEyeSlash\n                  onClick={() => setShowPassword(!showPassword)}\n                  className=\"mb-3\"\n                  size=\"20\"\n                />\n              )}\n            </div>\n\n            <p className=\"login-forget d-flex justify-content-center align-items-center\">\n              <FaLock className=\"mr-3\" />\n              Esqueceu sua senha?\n            </p>\n            <div className=\"mx-3 mt-3\">\n              <UiButton\n                loading={isLodding}\n                title=\"Entrar\"\n                type=\"submit\"\n                className=\"w-100 py-lg-2 py-1 login-btn\"\n              />\n            </div>\n          </form>\n        </div>\n\n        <div className=\"login-footer\">\n          <p className=\"login-text-left text-left pt-4\">Ou entre com</p>\n\n          <div className=\"mx-3 mt-3\">\n            <SocialButton\n              title=\"Google\"\n              icon={<FcGoogle />}\n              name=\"google\"\n              onClick={handleProvider}\n            />\n          </div>\n\n          <div className=\"mx-3 mt-3\">\n            <SocialButton\n              title=\"Facebook\"\n              icon={<ImFacebook />}\n              name=\"facebook\"\n              onClick={handleProvider}\n            />\n          </div>\n\n          <p className=\"login-bottom-text\">\n            NÃ£o tem uma conta?\n         